{
  "name": "schwobe.dev",
  "description": "https://schwobe.dev",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Michael Schwobe <mschwobe@gmail.com> (https://schwobe.dev)",
  "repository": "github:michaelschwobe/schwobe.dev",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "clean": "rimraf .cache/ build/ node_modules/.cache public/build/",
    "dev": "npm-run-all -s clean validate -p dev:*",
    "dev:remix": "cross-env NODE_ENV=development netlify dev",
    "fix": "run-s \"lint -- --fix\" format",
    "format": "prettier --ignore-unknown --write .",
    "lint": "eslint --ext=.js,.jsx,.ts,.tsx --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production netlify dev",
    "typecheck": "tsc -b",
    "validate": "run-s lint \"format -- --check --write=false\" typecheck"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "@remix-run/netlify": "^1.5.1",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "classcat": "^5.0.3",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  }
}
